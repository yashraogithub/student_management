<h2>Student List</h2>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Age</th>
      <th>Course</th>
    </tr>
  </thead>
  <tbody>
    {% for student in students %}
      <tr>
        <td>{{ student.id }}</td>
        <td>{{ student.name }}</td>
        <td>{{ student.age }}</td>
        <td>{{ student.course }}</td>
      </tr>
    {% else %}
      <tr>
        <td colspan="4">No students found.</td>
      </tr>
    {% endfor %}
  </tbody>
</table>
